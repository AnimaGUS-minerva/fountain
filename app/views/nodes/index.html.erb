<p id="notice"><%= notice %></p>

<h1>Listing Nodes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Fqdn</th>
      <th>Eui64</th>
      <th>Device type</th>
      <th>Manufacturer</th>
      <th>Idevid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nodes.each do |node| %>
      <tr>
        <td><%= node.name %></td>
        <td><%= node.fqdn %></td>
        <td><%= node.eui64 %></td>
        <td><%= node.device_type_id %></td>
        <td><%= node.manufacturer_id %></td>
        <td><%= node.idevid %></td>
        <td><%= link_to 'Show', node %></td>
        <td><%= link_to 'Edit', edit_node_path(node) %></td>
        <td><%= link_to 'Destroy', node, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Node', new_node_path %>
